{
  "builds": [
    {
      "src": "Gemfile",
      "use": "@vercel/ruby"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "s-maxage=86400"
      }
    }
  ],
  "scripts": {
    "build": "bundle install && bundle exec jekyll build"
  }
}
